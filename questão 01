#include <stdio.h>

int main() {
    int var1, var2;
    int *ptr1, *ptr2;

    // Inicializando variáveis
    var1 = 10;
    var2 = 20;

    // Atribuindo os endereços das variáveis aos ponteiros
    ptr1 = &var1;
    ptr2 = &var2;

    // Comparando os endereços
    if (ptr1 > ptr2) {
        printf("O endereco de var1 (%p) eh maior.\n", ptr1);
    } else if (ptr2 > ptr1) {
        printf("O endereco de var2 (%p) eh maior.\n", ptr2);
    } else {
        printf("Os enderecos sao iguais.\n");
    }

    return 0;
}
